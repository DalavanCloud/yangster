{
    "private": true,
    "name": "yangster-app",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "dependencies": {
        "font-awesome": "^4.7.0",
        "monaco-css": "^1.3.1",
        "monaco-editor": "^0.8.3",
        "monaco-editor-core": "^0.8.2",
        "monaco-html": "^1.2.1",
        "monaco-json": "^1.2.2",
        "monaco-languages": "^0.7.0",
        "monaco-typescript": "^2.1.2",
        "theia-yang-extension": "latest"
    },
    "localDependencies": {
        "theia-core": "../../theia",
        "sprotty": "../../sprotty/client",
        "theia-yang-extension": "../theia-yang-extension"
    },
    "devDependencies": {
    },
    "localDevDependencies": {
        "local-dependency-manager": "../../theia/config/local-dependency-manager"
    },
    "scripts": {
        "bootstrap": "npm install ../../theia/config/local-dependency-manager && npm run localinstall",
        "localinstall": "ldm install --dev --original-sources",
        "build": "npm run build:localdeps && npm run build:frontend",
        "build:localdeps": "ldm run build && ldm sync --original-sources",
        "watch": "concurrently --names watch-deps,watch-frontend --prefix \"[{name}]\" \"npm run watch:localdeps\" \"npm run watch:frontend\"",
        "watch:localdeps": "ldm watch --sync --run=watch --original-sources",
        "test:ui": "npm run start& wdio --waitforTimeout 300000 wdio.conf.js",
        "start:backend": "node ./src-gen/backend/main.js --root-dir=../workspace | bunyan",
        "start:backend:debug": "node ./src-gen/backend/main.js --root-dir=../workspace --loglevel=debug | bunyan"
    }
}